import java.util.*;

public class Ex_4 {

	public static void main(String[] args) {
		int a, b, c;
		int max = 0;
		
		System.out.println("세 개의 정수를 입력하세요");
		Scanner in = new Scanner(System.in);
		
		a = in.nextInt();
		b = in.nextInt();
		c = in.nextInt();
		
		if (a > b && a > c) {
			max = a;
		} else if (b > a && b > c) {
			max = b;
		} else if (c > a && c > b) {
			max = c;
		}
		
		System.out.printf("가장 큰 수는 %d입니다.", max);
	}

}
